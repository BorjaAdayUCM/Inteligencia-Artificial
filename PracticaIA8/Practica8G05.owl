<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.pizza.com/ontologies/pizza.owl#"
     xml:base="http://www.pizza.com/ontologies/pizza.owl"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.pizza.com/ontologies/pizza.owl">
        <rdfs:comment>A pizza ontology that describes various pizzas
based on their toppings.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isBaseOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasBebida -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasBebida">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Menu"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Bebidas"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasCountryOfOrigin -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasCountryOfOrigin">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasIngredient">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasPizza -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasPizza">
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Menu"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasPostre -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasPostre">
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Menu"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Postre"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SpicinessValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasToppingBebida -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasToppingBebida">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isToppingBebidaOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Bebidas"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#BebidasTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasToppingPostre -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasToppingPostre">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isToppingPostreOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Postre"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PostreTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaBase"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isToppingBebidaOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isToppingBebidaOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#BebidasTopping"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Bebidas"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isToppingPostreOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isToppingPostreOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Postre"/>
    </owl:ObjectProperty>
    


